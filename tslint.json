{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": false,
    "curly": false,
    "forin": false,
    "max-line-length": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "prefer-const": true,
    "quotemark": [true, "single"],
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "variable-name": [true, "ban-keywords", "check-format"]
  }
}
